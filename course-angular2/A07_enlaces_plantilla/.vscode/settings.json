// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
        "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/*.js.map": true,
        "**/node_modules/": true,
        "**/www/": true
    }
}